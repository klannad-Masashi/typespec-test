{
  "name": "typespec-test",
  "version": "1.0.0",
  "description": "TypeSpecを使用したWeb開発プロジェクト",
  "main": "index.js",
  "scripts": {
    "typespec:compile": "tsp compile typespec/tsp/main.tsp --output-dir temp/openapi",
    "generate:all": "npm run typespec:compile && python generator/main.py --target all",
    "generate:ddl": "python generator/main.py --target ddl",
    "generate:backend": "python generator/main.py --target spring", 
    "generate:frontend": "python generator/main.py --target angular",
    "generate:python": "python generator/main.py --target all",
    "setup:python": "pip install -r requirements.txt"
  },
  "devDependencies": {
    "@typespec/compiler": "^0.59.0",
    "@typespec/http": "^0.59.0",
    "@typespec/rest": "^0.59.0",
    "@typespec/openapi3": "^0.59.0",
    "js-yaml": "^4.1.0"
  },
  "author": "",
  "license": "MIT"
}